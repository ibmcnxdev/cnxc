{
    
    "name": "mmi - EViTA",
    "title": "mmi - EViTA",
    "description": "mmi - EViTA",
    "services": [
        "Customizer"
    ],
    "extensions": [
        {
            
            "name": "unHide New Community Button",
            "type": "com.ibm.customizer.ui",
            "payload": {
                "match": {
                    "user-name": [
                        "Michael Siegrist",
                        "Manfre Meise"
                    ]
                },
                "include-files": [
                    "EViTa/Config/Communites/unhideNewCommunityButton.js"
                ],
                "include-repo": {
                    "name": "cnxc"
                }
            },
            "path": "communities",
            "application": "mmi - EViTA"
        },
        {
            
            "name": "Add EViTA Config To User Profile",
            "type": "com.ibm.customizer.ui",
            "payload": {
                "match": {
                    "user-name": [
                        "Manfred Meise",
                        "Boris Eberlein",
                        "Michael Siegrist"
                    ]
                },
                "include-files": [
                    "EViTa/Config/Profile/ProfileConfig.js"
                ],
                "include-repo": {
                    "name": "cnxc"
                }
            },
            "path": "global",
            "application": "mmi - EViTA"
        },
		 {
            "id": "",
            "name": "Add EViTA Config into CNX Admin menu",
            "type": "com.ibm.customizer.ui",
            "payload": {
                "include-files": [
                    "EViTa/Config/AdminMenu/AdminMenuEntry.js"
                ],
                "include-repo": {
                    "name": "cnxc"
                }
            },
            "path": "global",
            "application": "Add EViTA Config into CNX Admin menu"
        },
		{
            "id": "9d3e0114-30fc-44c1-bf33-f9cbd744a1d0",
            "name": "Search Link Change",
            "type": "com.ibm.customizer.ui",
            "payload": {
                "include-files": [
                    "EViTa/Config/Search/OpenNewWindowSearch.js"
                ],
                "include-repo": {
                    "name": "cnxc"
                }
            },
            "path": "search",
            "application": "OpenNewWindowSearch",
            "state": "disabled"
        },
		{
            "id": "c47bcebd-f4a8-4805-bd05-c4d23c140f88",
            "name": "Smooth Loader",
            "type": "com.ibm.customizer.ui",
            "payload": {
                "include-files": [
                    "samples/smoothLoad/smoothLoad.js",
                    "samples/smoothLoad/smoothLoad.css"
                ],
                "include-repo": {
                    "name": "customizer"
                },
                "cache-headers": {
                    "cache-control": "max-age=0"
                }
            },
            "path": "global",
            "application": "smoothLoad",
            "state": "enabled"
        },
        {
            
            "name": "Add EViTA Config To Community",
            "type": "com.ibm.customizer.ui",
            "payload": {
                "match": {
					"include-files": [
                    "EViTa/Config/Communites/CommunityConfig.js"
                ],
                "include-repo": {
                    "name": "cnxc"
                }
            },
            "path": "global",
            "application": "mmi - EViTA",
            "state": "enabled"
        }
    ]
}