{
    "extensions": [{
            "path": "activities",
            "application": "Guest Model Package App",
            "payload": {
                "include-files": [
                    "EViTa/Config/Guests/commonTools.js",
                    "EViTa/Config/Guests/GuestModel_common.js",
                    "EViTa/Config/Guests/ActivityNoExternal.js"
                ]
            },
            "name": "Activity No External Extension",
            "description": "Prevents selected users from creating Activities open to external people",
            "type": "com.ibm.customizer.ui"
        },
        {
            "path": "global",
            "application": "Guest Model Package App",
            "payload": {
                "include-files": [
                    "EViTa/Config/Guests/commonTools.js",
                    "EViTa/Config/Guests/GuestModel_common.js",
                    "EViTa/Config/Guests/AddInviteGuest.js"
                ]
            },
            "name": "Add Invite Guest Extension",
            "description": "Adds te <Invite Guest> menu item for selected users",
            "type": "com.ibm.customizer.ui"
        },
        {
            "path": "communities",
            "application": "Guest Model Package App",
            "payload": {
                "match": {
                    "url": "communities/service/html/communitycreate"
                },
                "include-files": [
                    "EViTa/Config/Guests/commonTools.js",
                    "EViTa/Config/Guests/GuestModel_common.js",
                    "EViTa/Config/Guests/CommunityNoExternal.js"
                ]
            },
            "name": "Community No External Extension",
            "description": "Prevents selected users from creating Communities open to external people",
            "type": "com.ibm.customizer.ui"
        },
        {
            "path": "communities",
            "application": "Guest Model Package App",
            "payload": {
              "include-files": [
                "EViTa/Config/Guests/commonTools.js",
                "EViTa/Config/Guests/GuestModel_common.js",
                "EViTa/Config/Guests/CommunityNoMembers.js"
              ]
            },
            "name": "Community No Members",
            "description": "Prevents selected users from adding External Members to an existing Community",
            "type": "com.ibm.customizer.ui"
          },
          {
            "path": "files",
            "application": "Guest Model Restriction App",
            "payload": {
              "include-files": [
                "EViTa/Config/Guests/commonTools.js",
                "EViTa/Config/Guests/GuestModel_common.js",
                "EViTa/Config/Guests/FilesNoExternal.js"
              ]
            },
            "name": "Files No External",
            "description": "Prevents selected users from creating files or folders open to external people",
            "type": "com.ibm.customizer.ui"
          }
                ],
    "name": "Guest Model Restriction",
    "description": "Full Implementation of the Guest Model Restriction",
    "services": [
        "Customizer"
    ],
    "title": "Guest Model Restriction"
}